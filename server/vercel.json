{
    "version": 2,
    "builds": [
        {
            "src": "*server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src":"/user/(.*)",
            "dest":"/user/$1"
        },
        {
            "src":"/admin/(.*)",
            "dest":"/admin/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}